<ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/artikel/daftar">Artikel</a></li>
        <li class="breadcrumb-item active">tambah</li>
    </ol>
    <h1 class="display-4">Edit Artikel</h1>
    <form method="post" action="/artikel/update/<%= artikel.id %>">
        <div class="form-group">
            <label for="title">Title</label>
            <input type="text" name="title" class="form-control" value="<%= artikel.title %>">
        </div>
        <div class="form-group">
            <label for="body">body</label>
            <textarea name="body" class="form-control"><%= artikel.body %></textarea>
        </div>
        <input type="submit" value="submit" class="btn btn-primary">
    </form>